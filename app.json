{
  "expo": {
    "name": "Be Your Best",
    "slug": "be-your-best",
    "version": "1.0.0",
    "sdkVersion": "45.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.popAppMovile.beYourBestIOS",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.popAppMovile.beYourBestAndroid",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "backgroundColor": "#ffffff",
        "resizeMode": "native",
        "mdpi": "./assets/drawable/splashReact-mdpi.png",
        "hdpi": "./assets/drawable/splashReact-hdpi.png",
        "xhdpi": "./assets/drawable/splashReact-xhdpi.png",
        "xxhdpi": "./assets/drawable/splashReact-xxhdpi.png",
        "xxxhdpi": "./assets/drawable/splashReact-xxxhdpi.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
